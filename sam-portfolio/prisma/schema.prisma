// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model School{
  id        Int  @unique @id @default(autoincrement())
  name      String @unique
  major     String
  degree    String
  grad      String
}

model Projects{
  id    Int @unique @id @default(autoincrement())
  name  String
  site  String
}

model Clubs{
  id          Int  @unique @id @default(autoincrement())
  clubName    String 
  dateJoined  String
  position    String
}
